{
    "name": "guillaumebriday/laravel-blog",
    "description": "Laravel 10.x blog.",
    "keywords": ["framework", "laravel", "blog", "example"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-debugbar": "3.14.6",
        "doctrine/dbal": "4.2.1",
        "hotwired-laravel/turbo-laravel": "^2.1",
        "laravel/breeze": "^2.2.2",
        "laravel/framework": "^11.28",
        "laravel/horizon": "5.29.1",
        "laravel/sanctum": "^4.0.3",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "2.10.0",
        "nunomaduro/collision": "8.5.0",
        "predis/predis": "2.2.2",
        "pusher/pusher-php-server": "7.2.6",
        "spatie/laravel-backup": "9.1.2",
        "spatie/laravel-medialibrary": "11.9.2"
    },
    "require-dev": {
        "filp/whoops": "2.16.0",
        "fzaninotto/faker": "dev-master",
        "laravel/pint": "1.18.1",
        "mockery/mockery": "1.6.12",
        "phpunit/phpunit": "11.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/date.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
